<div class="form-group">
    <label>{{localize "DIALOG.ATTRIBUTE"}}</label>
    <div class="form-fields">
        <select name="system.transferData.avoidTest.attribute">
            {{#select system.transferData.avoidTest.attribute}}
                {{#each (config "attributes")}}
                    <option value="{{@key}}">{{this}}</option>
                {{/each}}
            {{/select}}
        </select>
    </div>
</div>
<div class="form-group">
    <label>{{localize "DIALOG.SKILL"}}</label>
    <div class="form-fields">
        <select name="system.transferData.avoidTest.skill">
            {{#select system.transferData.avoidTest.skill}}
                {{#each (config "skills")}}
                    <option value="{{@key}}">{{this}}</option>
                {{/each}}
            {{/select}}
        </select>
    </div>
</div>
<div class="form-group">
    <label>{{localize "DIALOG.DN"}}</label>
    <div class="form-fields">
        <input type="text" name="system.transferData.avoidTest.dn" value="{{system.transferData.avoidTest.dn}}">
    </div>
</div>