        <select name="system.transferData.avoidTest.attribute">
          {{selectOptions (config "attributes") selected=system.transferData.avoidTest.attribute blank=""}}
        </select>
    </div>
</div>
<div class="form-group">
    <label>{{localize "DIALOG.SKILL"}}</label>
